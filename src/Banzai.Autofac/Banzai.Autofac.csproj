<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>An easy Autofac extension for registering nodes in Banzai</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <NoWarn>NU1603</NoWarn>
    <AssemblyName>Banzai.Autofac</AssemblyName>
    <PackageId>Banzai.Autofac</PackageId>
    <PackageTags>Banzai Pipeline Dataflow Workflow Autofac</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="4.6.*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Banzai\Banzai.csproj" />
  </ItemGroup>

</Project>